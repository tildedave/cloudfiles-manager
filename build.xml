<?xml version="1.0"?>
<project basedir="." default="compile">

  <target name="plovr">
    <exec executable="java">
      <arg value="-jar" />
      <arg value="lib/plovr.jar" />
      <arg value="serve" />
      <arg value="plovr.js" />
    </exec>
  </target>

  <target name="server">
    <exec executable="ruby">
      <arg value="-rubygems" />
      <arg value="server.rb" />
    </exec>
  </target>

</project>






