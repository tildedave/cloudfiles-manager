<html>

  <head>
    <link type="text/css" rel="stylesheet" href="/stylesheets/main.css"></style>
    <script src="http://localhost:9810/compile?id=app"></script>
    <script>
      goog.require("ui.cloud_files.ContainerTable");
      goog.require("model.cache.ContainerProvider");

      function doit(){
         var testData = [
        { name: "Lucky",
          count: 154,
          bytes: 1000
        },
        { name: "Siggy",
          count: 20,
          bytes: 500 
        },
        { name: "Simon",
          count: 0,
          bytes: 123
        }
       ];

        var table = new ui.cloud_files.ContainerTable();
        var mockProvider = new model.cache.ContainerProvider(table);
        table.setProvider(mockProvider);
        mockProvider.setData(testData);
        table.load();
      }
    </script>
  </head>

  <body onload="doit()">
    <input type="button" value="Add Container" onclick="showAddContainerDialog()" />
    <div id="containers">
    </div>
  </body>
</html>
